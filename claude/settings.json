{
  "env": {
    "BASH_DEFAULT_TIMEOUT_MS": "300000",
    "CLAUDE_BASH_MAINTAIN_PROJECT_WORKING_DIR": "1",
    "CLAUDE_CODE_ENABLE_TELEMETRY": "0",
    "DISABLE_AUTOUPDATER": "1",
    "DISABLE_BUG_COMMAND": "1",
    "DISABLE_COST_WARNINGS": "1",
    "DISABLE_ERROR_REPORTING": "1",
    "DISABLE_NON_ESSENTIAL_MODEL_CALLS": "1",
    "DISABLE_TELEMETRY": "1"
  },
  "attribution": {
    "commit": "",
    "pr": ""
  },
  "permissions": {
    "allow": [
      "Bash(awk *)",
      "Bash(bd *)",
      "Bash(cat *)",
      "Bash(cp *)",
      "Bash(cut *)",
      "Bash(diff *)",
      "Bash(echo *)",
      "Bash(find *)",
      "Bash(gofmt *)",
      "Bash(grep *)",
      "Bash(hadolint *)",
      "Bash(head *)",
      "Bash(jq *)",
      "Bash(just *)",
      "Bash(ln:*)",
      "Bash(ls *)",
      "Bash(make *)",
      "Bash(mise *)",
      "Bash(mkdir *)",
      "Bash(mv *)",
      "Bash(pytest *)",
      "Bash(realpath *)",
      "Bash(rg *)",
      "Bash(ruff *)",
      "Bash(rustfmt *)",
      "Bash(sed *)",
      "Bash(semgrep *)",
      "Bash(shellcheck *)",
      "Bash(shfmt *)",
      "Bash(sort *)",
      "Bash(tail *)",
      "Bash(test *)",
      "Bash(touch *)",
      "Bash(ty *)",
      "Bash(type *)",
      "Bash(uniq *)",
      "Bash(wc *)",
      "Bash(which *)",
      "Bash(xargs *)",
      "Skill(superpowers:*)",
      "WebSearch",
      "mcp__atlassian__*",
      "mcp__ide__getDiagnostics",
      "mcp__firecrawl__*",
      "mcp__github__*",
      "mcp__plugin_context7_context7__*",
      "mcp__plugin_exa-mcp-server_exa__*",
      "mcp__plugin_playwright_playwright__*",
      "mcp__qmd__*"
    ],
    "deny": [
      "Bash(rm:*)",
      "Bash(rm -rf:*)",
      "Read(./.env)",
      "Read(./.env.*)",
      "Read(./secrets/**)",
      "Read(**/*credentials*)",
      "Read(**/*secret*)",
      "Read(**/*.pem)",
      "Read(**/*_rsa)",
      "Read(**/*_ed25519)"
    ],
    "defaultMode": "plan"
  },
  "plansDirectory": "./.claude/plans",
  "showTurnDuration": true,
  "statusLine": {
    "type": "command",
    "command": "~/.claude/statusline",
    "padding": 0
  },
  "extraKnownMarketplaces": {
    "beads-marketplace": {
      "source": {
        "source": "github",
        "repo": "steveyegge/beads"
      }
    },
    "cctools-plugins": {
      "source": {
        "source": "github",
        "repo": "pchalasani/claude-code-tools"
      }
    },
    "exa-mcp-server": {
      "source": {
        "source": "github",
        "repo": "exa-labs/exa-mcp-server"
      }
    },
    "superpowers-marketplace": {
      "source": {
        "source": "github",
        "repo": "obra/superpowers"
      }
    },
    "trailofbits": {
      "source": {
        "source": "github",
        "repo": "trailofbits/skills"
      }
    }
  },
  "enabledPlugins": {
    "beads@beads-marketplace": true,
    "claude-md-management@claude-plugins-official": true,
    "claude-code-setup@claude-plugins-official": true,
    "context7@claude-plugins-official": true,
    "exa-mcp-server@exa-mcp-server": true,
    "playwright@claude-plugins-official": true,
    "safety-hooks@cctools-plugins": true,
    "superpowers@superpowers-marketplace": true
  }
}
