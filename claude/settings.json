    "deny": [
      "Read(./.env)",
      "Read(./.env.*)",
      "Read(./secrets/**)",
      "Read(**/*credentials*)",
      "Read(**/*secret*)",
      "Read(**/*.pem)",
      "Read(**/*_rsa)",
      "Read(**/*_ed25519)"
    ],
    "defaultMode": "plan"
  },
  "hooks": {
    "SessionStart": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "printf '[CONTEXT] Now: %s\\n' \"$(date +'%Y-%m-%d %H:%M:%S %Z')\""
          },
          {
            "type": "command",
            "command": "~/.claude/hooks/homunculus-start/hook",
            "async": true
          }
        ]
      }
    ],
    "PreToolUse": [
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": "~/.claude/hooks/tmux-guard/hook"
          },
          {
            "type": "command",
            "command": "~/.claude/hooks/trash-guard/hook"
          },
          {
            "type": "command",
            "command": "~/.claude/hooks/grep-guard/hook"
          },
          {
            "type": "command",
            "command": "~/.claude/hooks/find-guard/hook"
          },
          {
            "type": "command",
            "command": "~/.claude/hooks/bq-query-guard/hook"
          },
          {
            "type": "command",
            "command": "~/.claude/hooks/bd-create-guard/hook"
          }
        ]
      },
      {
        "matcher": "WebFetch",
        "hooks": [
          {
            "type": "command",
            "command": "~/.claude/hooks/web-fetch-guard/hook"
          }
        ]
      },
      {
        "matcher": "WebSearch",
        "hooks": [
          {
            "type": "command",
            "command": "~/.claude/hooks/web-search-guard/hook"
          }
        ]
      },
      {
        "matcher": "Edit|Write|MultiEdit|NotebookEdit",
        "hooks": [
          {
            "type": "command",
            "command": "~/.claude/hooks/worktree-guard/hook"
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "Write|Edit|MultiEdit",
        "hooks": [
          {
            "type": "command",
            "command": "~/.claude/hooks/britfix/hook",
            "async": true
          },
          {
            "type": "command",
            "command": "~/.claude/hooks/homunculus-recall/hook",
            "async": true
          }
        ]
      },
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "~/.claude/hooks/homunculus-observe/hook",
            "async": true
          }
        ]
      }
    ],
    "Notification": [
      {
        "matcher": "permission_prompt",
        "hooks": [
          {
            "type": "command",
            "command": "~/.claude/hooks/notify/hook",
            "async": true
          }
        ]
      }
    ],
    "Stop": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "~/.claude/hooks/notify/hook",
            "async": true
          },
          {
            "type": "command",
            "command": "~/.claude/hooks/homunculus-stop/hook",
            "async": true
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "~/.claude/statusline",
    "padding": 0
  },
  "enabledPlugins": {
    "beads@beads-marketplace": true,
    "claude-code-setup@claude-plugins-official": true,
    "claude-md-management@claude-plugins-official": true,
    "differential-review@trailofbits": true,
    "insecure-defaults@trailofbits": true,
    "marketing-skills@marketingskills": true,
    "pyright-lsp@claude-plugins-official": true,
    "sharp-edges@trailofbits": true,
    "static-analysis@trailofbits": true,
    "superpowers@superpowers-marketplace": true
  },
  "extraKnownMarketplaces": {
    "beads-marketplace": {
      "source": {
        "source": "github",
        "repo": "steveyegge/beads"
      }
    },
    "dbt-agent-marketplace": {
      "source": {
        "source": "github",
        "repo": "dbt-labs/dbt-agent-skills"
      }
    },
    "superpowers-marketplace": {
      "source": {
        "source": "github",
        "repo": "obra/superpowers"
      }
    },
    "trailofbits": {
      "source": {
        "source": "github",
        "repo": "trailofbits/skills"
      }
    }
  },
  "outputStyle": "Personal",
  "plansDirectory": "./.claude/plans",
  "showTurnDuration": true
}
